{
  "expo": {
    "name": "logger-generator-native",
    "slug": "logger-generator-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./pictures/logo-transparent.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./pictures/logo-generators.png"
    }
  }
}
